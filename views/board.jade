doctype html
html
  include ./includes/head.jade
  body
    h1!= h1
    main
      each val in threads
        section.thread
          header
            p.postID= 'No.' + val.postID
            p.title= val.title
            p.name= val.name
          section.thumb
            img(src= 'media/img/' + val.string)/
          section.message
            p= val.message